{
  "version": "1.0.0",
  "background": {
    "scripts": ["dist/background.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["dist/content.js"]
    }
  ],
  "web_accessible_resources": ["dist/*"],
  "manifest_version": 2,
  "name": "webextension-target",
  "description": "webextension target",
  "permissions": ["<all_urls>"]
}
